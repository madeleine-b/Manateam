{
  "manifest_version": 2,

  "name": "Customize Color",
  "description": "Modify webpages to personalized colorschemes",
  "version": "1.0",
  
  "background": {
    "scripts": ["background.js"],
    "persistant": false
  },

 "options_page": "options/index.html",

 "content_scripts": [
    {
      "matches": ["<all_urls>"]
      "css": ["mystyles.css"]
      "js": ["jquery-2.1.3.js", 
            "colormanip.js"]
    }
  ],
{
    "matches": ["<all_urls>"],

    "css": [
    ],

    "js": [
    ],

    "run_at": "document_start",
    "all_frames": true
  },

"browser_action": {
    "default_icon": "icon.png",
    "default_title": "Customize Color"
    "default_popup": "popup.html"
  },

  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  },

  "permissions": [
    "activeTab"
    "<all_urls>"
  ]
}